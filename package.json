{
  "name": "textbook-backend",
  "version": "1.0.0",
  "description": "Magic link booking system with Supabase integration and Prisma ORM",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "dev:build": "tsc --watch",
    "test": "node dist/test/api-test.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "@prisma/client": "^5.8.1",
    "nanoid": "^5.0.4",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "typescript": "^5.3.3",
    "nodemon": "^3.0.2",
    "prisma": "^5.8.1",
    "tsx": "^4.7.0",
    "ts-node": "^10.9.2"
  },
  "keywords": ["express", "typescript", "prisma", "supabase", "booking", "magic-link", "nanoid"],
  "author": "",
  "license": "MIT"
} 